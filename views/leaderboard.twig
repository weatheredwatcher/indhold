<!DOCTYPE HTML>

<html>

<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<title>Enterprise 2016 Video Wall</title>
	
	<!-- Scripts -->
	<script src="/assets/leaderboard/js/jquery-3.2.1.min.js"></script>
	<script src="/assets/leaderboard/js/functions.js"></script>
	<script src="/assets/leaderboard/js/isotope.pkgd.min.js"></script>
	<script src="/assets/leaderboard/js/packery-mode.pkgd.js"></script>
	<script src="/assets/leaderboard/js/jquery.cycle2.min.js"></script>
	<script src="/assets/leaderboard/js/jquery.cycle2.scrollVert.min.js"></script>
	<script src="/assets/leaderboard/js/scoreboard.js"></script>

	<!-- Tyepkit -->
	<script src="https://use.typekit.net/oae8zyf.js"></script>
	<script>try{Typekit.load({ async: true });}catch(e){}</script>
	<!-- CSS -->
	<link rel="stylesheet" href="/assets/leaderboard/css/styles.css" />
	
</head>

<body>

<header><img src="/assets/leaderboard/img/ew-games-logo.png" alt="The EW Games" /></header>
	<!-- Carousel Container for all slides on video wall
	<div id="videoWall" data-cycle-fx="scrollHorz" data-cycle-slides="> div"> -->
		<div class="cycle-slideshow" data-cycle-fx="scrollHorz" data-cycle-slides="> div" data-cycle-pager=".pager">

		<!-- Social Wall -->
		<div id="socialWall" class="slide" data-cycle-timeout="12000" data-cycle-slides="> div">

			<div id="socialWallContainer">


			</div>

		</div><!-- Social Wall -->
		
		<!-- Overall Leaderboard -->
		<div id="leaderboardOverall" class="slide" data-cycle-timeout="7000">
			
			<!-- Title -->
			<div id="titleBlock">
				
				<div id="headings">
					<h2>LEADERBOARD</h2>
					<h1>Overall by Team</h1>
				</div>
				<img src="/assets/leaderboard/img/ew-games-logo.png" />
				
			</div>
		
			<div id="overallLeaderboard">
				<div class="rankings">
					<span>1</span>
					<span>2</span>
					<span>3</span>
					<span>4</span>
					<span>5</span>
				</div>
				<div class="rowAlt">
					<span class="even"></span>
					<span></span>
					<span class="even"></span>
					<span></span>
					<span class="even"></span>
				</div>
				<div class="teamRow">
					<span class="rank blue"></span>
					<span class="icon bl"></span>
					<span class="teamName">Team Blue</span>
					<span class="points" id="blPoints"></span>
				</div>
				<div class="teamRow">
					<span class="rank purple"></span>
					<span class="icon pl"></span>
					<span class="teamName">Team Purple</span>
					<span class="points" id="plPoints"></span>
				</div>
				<div class="teamRow">
					<span class="rank green"></span>
					<span class="icon gr"></span>
					<span class="teamName">Team Green</span>
					<span class="points" id="grPoints"></span>
				</div>
				<div class="teamRow">
					<span class="rank orange"></span>
					<span class="icon or"></span>
					<span class="teamName"> Team Orange</span>
					<span class="points" id="orPoints"></span>
				</div>
				<div class="teamRow">
					<span class="rank teal"></span>
					<span class="icon tl"></span>
					<span class="teamName"> Team Teal</span>
					<span class="points" id="tlPoints"></span>
				</div>
			</div>
			
		</div><!-- End Overall Leaderboard -->
		
		
		
		<!-- Individual Leaderboard -->
		<div id="leaderboardIndividual" class="slide" data-cycle-timeout="7000">
			
			<!-- Title -->
			<div id="titleBlock">
				
				<div id="headings">
					<h2>LEADERBOARD</h2>
					<h1>Overall by Individual</h1>
				</div>
				<img src="/assets/leaderboard/img/ew-games-logo.png" />
				
			</div>
		
			<div id="individualLeaderboard">
				<div class="rankings">
					<span>1</span>
					<span>2</span>
					<span>3</span>
					<span>4</span>
					<span>5</span>
				</div>
				<div class="rowAlt">
					<span class="even"></span>
					<span></span>
					<span class="even"></span>
					<span></span>
					<span class="even"></span>
				</div>
				
			</div>
			
		</div><!-- End Overall Leaderboard -->
		
		
		<div class="winnerDraw slide"  data-cycle-timeout="7000">
			<img src="/assets/leaderboard/img/winner-draw3.png" />
		</div>
		
		<div class="winnerDraw slide"  data-cycle-timeout="7000">
			<img src="/assets/leaderboard/img/winner-draw4.png" />
		</div>
		
		
		
